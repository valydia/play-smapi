<?xml version="1.0" encoding="utf-8" ?>
<Presentation>
    <!-- BrowseOptions allow for an override of standard browse operations in the controller -->
    <!-- <BrowseOptions PageSize="250" /> -->
    <PresentationMap type="Search">
        <Match>
            <SearchCategories>
                <!-- providers that use the canonical identifiers -->
                <Category id="artists"/>
                <Category id="albums"/>
                <Category id="tracks"/>
            </SearchCategories>
        </Match>
    </PresentationMap>
